{
  "name": "evokepass",
  "version": "1.0.0",
  "description": "Multi-module system with TCP server and Web UI",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start:tcp": "node dist/tcp-server/index.js",
    "start:web": "node dist/web-ui/index.js",
    "dev:tcp": "ts-node src/tcp-server/index.ts",
    "dev:web": "ts-node src/web-ui/index.ts",
    "dev": "concurrently \"npm run dev:tcp\" \"npm run dev:web\"",
    "start": "concurrently \"npm run start:tcp\" \"npm run start:web\""
  },
  "keywords": [
    "tcp",
    "web-ui",
    "database"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "express": "^4.18.2",
    "node-fetch": "^2.7.0",
    "onvif": "^0.2.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/node-fetch": "^2.6.13",
    "@types/xml2js": "^0.4.14",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
